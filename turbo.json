{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "lint": {},
    "start": {
      "dependsOn": [
        "build",
        "wait_for_client_build"
      ]
    },
    "wait_for_client_build": {},
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
