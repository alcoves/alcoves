{
  "$schema": "https://typedoc.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "schema"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "schema": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
