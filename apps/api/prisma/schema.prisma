generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("ALCOVES_API_DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model UserSession {
  id        String   @id @default(uuid())
  userId    String
  ip        String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
