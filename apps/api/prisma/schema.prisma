generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("ALCOVES_DATABASE_URL")
}

model Image {
  id             String   @id @default(uuid())
  name           String   @default("")
  input          String
  content_type   String
  storage_bucket String
  storage_key    String
  updatedAt      DateTime @updatedAt
  createdAt      DateTime @default(now())
}
