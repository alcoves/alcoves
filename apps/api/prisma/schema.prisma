generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("ALCOVES_DATABASE_URL")
}

model Asset {
  id             String   @id @default(uuid())
  input          String
  name           String
  storage_bucket String
  storage_key    String
  updatedAt      DateTime @updatedAt
  createdAt      DateTime @default(now())
}
