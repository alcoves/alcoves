        "@aws-sdk/s3-request-presigner": "^3.670.0",
        "@hono/zod-validator": "^0.2.2",
        "@lucia-auth/adapter-prisma": "^4.0.1",
        "@prisma/client": "^5.21.0",
        "bullmq": "^5.20.0",
        "google-auth-library": "^9.14.2",
        "hono": "^4.6.5",
        "lucia": "^3.2.1",
        "pg": "^8.13.0",
        "postgres": "^3.4.4",
        "sharp": "^0.33.5",
        "uuid": "^10.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/pg": "^8.11.10",
        "@types/uuid": "^10.0.0",
        "prisma": "^5.21.0"
    }
