[
  {
    "name": "web",
    "image": "${app_image}",
    "cpu": 256,
    "memory": 512,
    "stopTimeout": 10,
    "startTimeout": 10,
    "portMappings": [
      {
        "containerPort": 3000
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
